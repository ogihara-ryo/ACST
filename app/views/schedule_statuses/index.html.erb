<h1>Listing schedule_statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedule_statuses.each do |schedule_status| %>
      <tr>
        <td><%= schedule_status.name %></td>
        <td><%= link_to 'Show', schedule_status %></td>
        <td><%= link_to 'Edit', edit_schedule_status_path(schedule_status) %></td>
        <td><%= link_to 'Destroy', schedule_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule status', new_schedule_status_path %>
