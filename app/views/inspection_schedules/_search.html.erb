<div class="well col-md-15 col-sm-15 col-xs-15">
  <%= search_form_for @search, url: { action: params[:action] } do |f| %>
    <div>
      <p>
        <%= t('activerecord.attributes.inspection_schedule.service_id') %>
        <%= f.collection_select(:service_id_eq, Service.all, :id, :name, :include_blank => true ) %>
        <%= t('activerecord.attributes.inspection_schedule.schedule_status_id') %>
        <%= f.collection_select(:schedule_status_id_eq, ScheduleStatus.all, :id, :name, :include_blank => true ) %>
        <%= t('activerecord.attributes.equipment.place_id') %>
        <%= f.collection_select(:equipment_place_id_eq, Place.all, :id, :name, :include_blank => true ) %>
        <%= t('activerecord.attributes.inspection_schedule.target_yearmonth') %>
        <%= f.text_field :target_yearmonth_eq %>
      </p>
    </div>
    <div>
      <%= f.submit t('helpers.submit.search') %>
    </div>
  <% end %>
</div>
